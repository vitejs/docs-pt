{
  "name": "vite-docs-pt",
  "version": "1.0.0",
  "description": "Documentação da Vite em Português",
  "type": "module",
  "main": "docs/index.md",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18"
      },
      "ignoreMissing": [
        "search-insights",
        "postcss",
        "@algolia/client-search"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitejs/docs-pt.git"
  },
  "keywords": [
    "vite-pt",
    "vite-docs-pt",
    "vite-portuguese-translation",
    "vite-pt-br",
    "vite-pt-pt",
    "vite-portuguese-documentation",
    "pt-pt",
    "pt-br",
    "pt"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vitejs/docs-pt/issues"
  },
  "homepage": "https://github.com/vitejs/docs-pt#readme",
  "dependencies": {
    "vitepress": "1.0.0-rc.24"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/node": "^20.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
